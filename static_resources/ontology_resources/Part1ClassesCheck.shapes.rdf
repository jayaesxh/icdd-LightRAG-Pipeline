    <sh:sparql rdf:parseType="Resource">
      <sh:select>prefix rdfs:&lt;http://www.w3.org/2000/01/rdf-schema#&gt;&#xD;
prefix owl:&lt;http://www.w3.org/2002/07/owl#&gt;&#xD;
&#xD;
SELECT distinct $this
WHERE &#xD;
   {
     $this a owl:Class . FILTER not exists { $this a &lt;https://standards.iso.org/iso/21597/-1/ed-1/en/AnnexD/Part1Schema#ICDDClass&gt;} . FILTER isIRI ($this) .&#xD;
	}&#xD;
</sh:select>
      <sh:message>{$this} class extension is not allowed.</sh:message>
      <sh:prefixes rdf:resource=""/>
    </sh:sparql>
    <sh:targetClass rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/ns/shacl#NodeShape"/>
  </rdfs:Class>
